<ul class='truck-results'>
  <% trucks.forEach(function (truck) { %>
    <li class='truck' data-title='<%= truck.escape('id') %>'>
      <p class='lead'><%= truck.escape('name') %></p>
      <p class='address'><%= truck.escape('address') %></p>
      <p><%= truck.escape('food_items') %></p>
    </li>
  <% }); %>
</ul>